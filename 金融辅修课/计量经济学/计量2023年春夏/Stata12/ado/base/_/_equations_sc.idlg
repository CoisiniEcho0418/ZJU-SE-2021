PROGRAM _equations_output
BEGIN
	if !main.vl_dv1 {
		stopbox stop `""Dependent variables for equation 1""'
			'"must be filled in"'
	}

	if main.vl_dv1 {
		put "("
		put /program eq1_output
		put ") "
	}
	if main.vl_dv2 {
		put "("
		put /program eq2_output
		put ") "
	}
	if main.vl_dv3  {

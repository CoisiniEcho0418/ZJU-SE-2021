PROGRAM eq1_output
BEGIN
	if main.ed_eqn1 {
		put /hidden main.ed_eqn1 ": "
	}
	put /hidden main.vl_dv1
	if main.vl_iv1 {
		put " "  "= "
		put /hidden main.vl_iv1
	}
	beginoptions
		hoption main.ck_nocon1
	endoptions
END


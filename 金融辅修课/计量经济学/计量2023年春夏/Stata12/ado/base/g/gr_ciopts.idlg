		gr_ciopts_rconn5Results | gr_ciopts_rconn6Results |	///
		gr_ciopts_rconn7Results | gr_ciopts_rconn8Results |	///
		gr_ciopts_rconnmdResults |				///
		gr_ciopts_rconnm1Results |				///
		gr_ciopts_rconnm2Results |				///
		gr_ciopts_rconnm3Results |				///
		gr_ciopts_rconnm4Results |				///
		gr_ciopts_rconnm5Results |				///
		gr_ciopts_rconnm6Results |				///
		gr_ciopts_rconnm7Results | gr_ciopts_rconnm8Results) {
			call program gr_ciopts_rconnect_output
		}
	}

	if gr_ciopts.cb_plot_type.isneq("") {
		optionarg gr_ciopts.cb_plot_type
	}

	put gr_ciopts_rcapOutput
	put gr_ciopts_rareaOutput
	put gr_ciopts_rbarOutput
	put gr_ciopts_rspikeOutput
	put gr_ciopts_rcapsymOutput
	put gr_ciopts_rscatterOutput
	put gr_ciopts_rlineOutput
	put gr_ciopts_rconnectOutput

		if (gr_ciopts_rlinedResults |				///
		gr_ciopts_rline1Results | gr_ciopts_rline2Results |	///
		gr_ciopts_rline3Results | gr_ciopts_rline4Results |	///
		gr_ciopts_rline5Results | gr_ciopts_rline6Results |	///
		gr_ciopts_rline7Results | gr_ciopts_rline8Results) {
			call program gr_ciopts_rline_output
		}
	}


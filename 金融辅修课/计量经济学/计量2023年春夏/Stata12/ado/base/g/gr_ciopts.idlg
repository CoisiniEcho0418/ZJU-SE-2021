	call gr_ciopts_rcapOutput.setvalue ""
	call gr_ciopts_rareaOutput.setvalue ""
	call gr_ciopts_rbarOutput.setvalue ""
	call gr_ciopts_rspikeOutput.setvalue ""
	call gr_ciopts_rcapsymOutput.setvalue ""
	call gr_ciopts_rscatterOutput.setvalue ""
	call gr_ciopts_rlineOutput.setvalue ""
	call gr_ciopts_rconnectOutput.setvalue ""

	if gr_ciopts.cb_plot_type.iseq("") {
		if (gr_ciopts_rcapdResults |				///
		gr_ciopts_rcap1Results | gr_ciopts_rcap2Results |	///
		gr_ciopts_rcap3Results | gr_ciopts_rcap4Results |	///
		gr_ciopts_rcap5Results | gr_ciopts_rcap6Results |	///
		gr_ciopts_rcap7Results | gr_ciopts_rcap8Results |	///
		gr_ciopts.cb_rcap_d | gr_ciopts.cb_rcap_1 |		///
		gr_ciopts.cb_rcap_2 | gr_ciopts.cb_rcap_3 |		///
		gr_ciopts.cb_rcap_4 | gr_ciopts.cb_rcap_5 |		///
		gr_ciopts.cb_rcap_6 | gr_ciopts.cb_rcap_7 |		///
		gr_ciopts.cb_rcap_8) {
			call program gr_ciopts_rcap_output
		}
	}

	if gr_ciopts.cb_plot_type.iseq("rarea") {
		if (gr_ciopts_rareadResults |				///
		gr_ciopts_rarea1Results | gr_ciopts_rarea2Results |	///
		gr_ciopts_rarea3Results | gr_ciopts_rarea4Results |	///
		gr_ciopts_rarea5Results | gr_ciopts_rarea6Results |	///
		gr_ciopts_rarea7Results | gr_ciopts_rarea8Results) {
			call program gr_ciopts_rarea_output
		}
	}

	if gr_ciopts.cb_plot_type.iseq("rbar") {
		if (gr_ciopts_rbardResults | gr_ciopts_rbar1Results |	///
		gr_ciopts_rbar2Results | gr_ciopts_rbar3Results |	///
		gr_ciopts_rbar4Results | gr_ciopts_rbar5Results |	///
		gr_ciopts_rbar6Results | gr_ciopts_rbar7Results |	///
		gr_ciopts_rbar8Results) {
			call program gr_ciopts_rbar_output
		}
	}

	if gr_ciopts.cb_plot_type.iseq("rspike") {
		if (gr_ciopts_rspikedResults |				///
		gr_ciopts_rspike1Results | gr_ciopts_rspike2Results |	///
		gr_ciopts_rspike3Results | gr_ciopts_rspike4Results |	///
		gr_ciopts_rspike5Results | gr_ciopts_rspike6Results |	///
		gr_ciopts_rspike7Results | gr_ciopts_rspike8Results) {
			call program gr_ciopts_rspike_output
		}
	}
	if gr_ciopts.cb_plot_type.iseq("rcapsym") {
		if (gr_ciopts_rcapsdResults |				///
		gr_ciopts_rcaps1Results | gr_ciopts_rcaps2Results |	///
		gr_ciopts_rcaps3Results | gr_ciopts_rcaps4Results |	///
		gr_ciopts_rcaps5Results | gr_ciopts_rcaps6Results |	///
		gr_ciopts_rcaps7Results | gr_ciopts_rcaps8Results |	///
		gr_ciopts_rcapsmdResults | gr_ciopts_rcapsm1Results |	///
		gr_ciopts_rcapsm2Results | gr_ciopts_rcapsm3Results |	///
		gr_ciopts_rcapsm4Results | gr_ciopts_rcapsm5Results |	///
		gr_ciopts_rcapsm6Results | gr_ciopts_rcapsm7Results |	///
		gr_ciopts_rcapsm8Results) {
			call program gr_ciopts_rcapsym_output
		}
	}

	if gr_ciopts.cb_plot_type.iseq("rscatter") {
		if (gr_ciopts_rscatdResults |				///
		gr_ciopts_rscat1Results | gr_ciopts_rscat2Results |	///
		gr_ciopts_rscat3Results | gr_ciopts_rscat4Results |	///

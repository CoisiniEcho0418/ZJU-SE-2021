BEGIN
	create CHILD gr_over_options AS over2, allowsubmit
	over2.setExitString over2Results
	over2.setOkAction "program check_over2Results"
	over2.setSubmitAction "script over2Submit"
END
PROGRAM check_over2Results
BEGIN

PROGRAM gr_over1_options_output
BEGIN
	beginoptions
		put over1Results
	endoptions
END
PROGRAM gr_over2_options_output
BEGIN
	beginoptions
		put over2Results
	endoptions
END
PROGRAM gr_over3_options_output
BEGIN
	beginoptions
		put over3Results
	endoptions
END

PROGRAM gr_over_output
BEGIN
	if gr_over.ck_over1 {
		require gr_over.vn_var1
		put " over("
		put gr_over.vn_var1
		put /program gr_over1_options_output
		put ") "
	}
	if gr_over.ck_over2 {
		require gr_over.vn_var2
		put " over("
		put gr_over.vn_var2
		put /program gr_over2_options_output
		put ") "
	}
	if gr_over.ck_over3 {
		require gr_over.vn_var3
		put " over("
		put gr_over.vn_var3
		put /program gr_over3_options_output
		put ") "
	}
END

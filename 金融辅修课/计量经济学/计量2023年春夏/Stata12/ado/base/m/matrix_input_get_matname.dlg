VERSION 12.0

INCLUDE _std_small
DEFINE _dlght 100
INCLUDE header

DIALOG main, title("Load matrix with constant")
BEGIN
  TEXT     tx_mat	_lft	_top	_iwd	.,		///
	label("Choose a matrix:")
  COMBOBOX cb_mat	_lft	_ss	_cwd1	.,		///
	label("Choose a matrix")				///
	contents(matrix) dropdown
END

PROGRAM command
BEGIN
	require main.cb_mat
	put main.cb_mat
END

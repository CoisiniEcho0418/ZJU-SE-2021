				}
				put " "
			}
			if main.cb_meth4 {
				put " " main.cb_meth4
				if main.sp_perc4 {
					put main.sp_perc4
				}
				if main.vn_mvar4 {
					put " "  main.vn_mvar4
				}
				put " "
			}
			if main.cb_meth5 {

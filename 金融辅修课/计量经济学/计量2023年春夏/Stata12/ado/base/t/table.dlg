		*/ option("scsepwidth")					/*
		*/ default("0")						/*
		*/ numonly						/*
		*/ label("Supercolumn-sep. width")
  TEXT     tx_scsep     _en7sep        @ _cken7r2        .,		/*
		*/ label("Supercolumn-sep. width")

  TEXT     tx_alignment    _lft      _ls   _cwd1         .,		/*
		*/ label("Alignment in cells:")

		*/ numonly						/*
		*/ label("Stub width")
  TEXT     tx_stub      _en7sep        @ _cken7r2        .,		/*
		*/ label("Stub width")
  CHECKBOX ck_scsep        _lft      _ms   _ckwd         .,  		/*
		*/ clickon("gaction opt.en_scsep.enable")		/*
		*/ clickoff("gaction opt.en_scsep.disable")		/*
		*/ label("")

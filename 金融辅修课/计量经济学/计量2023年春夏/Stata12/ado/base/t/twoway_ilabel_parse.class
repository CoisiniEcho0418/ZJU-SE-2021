//                                 twoway_ilabel
//
//   A parser for adding a immediate labels.  Works with twoway.ado.

*! version 1.0.0  25oct2002

version 8

class {
} , inherit(twoway_ipoint_parse)



// ---------------------------------------------------------------------------

program log_edits
	args log view i

							// defaults
	.`log'.Arrpush .`view'.numkeys = 0
	.`log'.Arrpush .`view'.style.setstyle , style(scheme ilabel)

	.Super.log_edits `0'
end

// Instrument 1
PROGRAM main_1_on
BEGIN
	call main.vl_level_1.enable
	call main.ck_lags_1.enable
	if main.ck_lags_1 {
		call main.ed_lags_1.enable
	}
END
SCRIPT main_1_off
BEGIN
	main.vl_level_1.disable
	main.ck_lags_1.disable
	main.ed_lags_1.disable
END
PROGRAM main_lag1_on
BEGIN
	if main.ck_lags_1.isenabled() {
		call main.ed_lags_1.enable
	}
END


VERSION 11.0

INCLUDE _std_large
DEFINE _dlght 290
INCLUDE header_gr_child

DEFINE c1 20
DEFINE c1wd 70

DEFINE c2 95
DEFINE c2wd 230

DEFINE c3h 355
DEFINE c3hwd 120
DEFINE c3 355


HELP hlp1, view("help xtdpd")
RESET res1


DIALOG main, tabtitle("Main")	///
	label("GMM-type instruments for the level equation")
BEGIN
  GROUPBOX gb_instruments	_lft	_top	_iwd	_ht23h,		///
	label("GMM-type instruments for the level equation")


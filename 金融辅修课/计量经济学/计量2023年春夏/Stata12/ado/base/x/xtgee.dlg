  VARNAME  vn_offset _ilft4_2  @         @         .,			/*
		*/ label("Offset variable")				/*
		*/ option("offset")					/*
		*/

  CHECKBOX ck_nocons _ilft     _ls       _cwd2     .,			/*
		*/ label("Suppress constant term")			/*
